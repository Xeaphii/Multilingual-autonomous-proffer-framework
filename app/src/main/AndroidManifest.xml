<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="androidassignment.crossover.com.androidassignment">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddAuctionItem"
            android:label="@string/add_auction_item"></activity>
        <activity
            android:name=".PlaceBid"
            android:label="@string/place_bid"></activity>
        <activity
            android:name=".EditAuctionItem"
            android:label="@string/modify_auction_item"></activity>

    </application>

</manifest>
